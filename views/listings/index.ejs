<% layout("/layouts/boilerplate.ejs") %>
  <body>
    <h2>All Images</h2>
    <div class="row row-col-lg-4 row-col-md-2 row-col-sm-1 ">
    <% for(let listing of alllisting) {%>
      
    <div class="card" style="width: 20rem; ">
      <a href="/listing/<%= listing._id%> " class="listing-link">
      <img src="<%= listing.image.url %>" class="card-img-top" alt="listing_image" style="height: 18rem ">
      <div class="card-img-overlay">new</div>
      <div class="card-body">
        <p class="card-text">
          <b><%=listing.title%></b><br>
          &#8377 <%=listing.price.toLocaleString("en-IN")%> / Day
        </p>
      </div>
    </a>
    </div>
     
    <%}%>
  </div>
    
  </body>

